
<DxChart Data="@resultsDictionarys">
    <DxChartTitle Text="Total Sales" />
    <DxChartLegend  AllowToggleSeries="true" VerticalAlignment="@VerticalEdge.Bottom" Position="@RelativePosition.Outside" />
    <DxChartBarSeries ValueField="@((GraphicSearch i) => i.Value)" ArgumentField="@(i => i.Column)" AggregationMethod="@Enumerable.Sum">
        <DxChartSeriesLabel Visible="true" />
    </DxChartBarSeries>
</DxChart>
<hr />
<button @onclick="Cancel" type="button" class="btn btn-secondary">Close</button>

 
@code {
    [CascadingParameter]
    public BlazoredModalInstance ModalInstance { get; set; }
    [CascadingParameter]
    public ModalParameters Parameters { get; set; }
    public List<GraphicSearch> resultsDictionarys { get; set; }
    public string Title { get; set; }


    protected override void OnInitialized()
    {
        resultsDictionarys = Parameters.Get<List<GraphicSearch>>("resultsDictionarys");
        Title = Parameters.Get<string>("title");

        base.OnInitialized();
    }

    public void Cancel()
    {
        ModalInstance.Cancel();
    }
}
